    "import ir_datasets\n",
    "import pandas as pd\n",
    "dataset = ir_datasets.load(\"neuclir/1/multi/trec-2023\")\n",
    "\n",
    "sum = 0\n",
    "for doc in dataset.docs_iter():\n",
    "    sum += len(doc.title)\n",
    "\n",
    "avg_title_length = sum / dataset.docs_count()\n",
    "\n",
    "print(avg_title_length)"
